{
  "_moduleAliases": {
    "src": "./dist"
  },
  "author": "Milos Bugarinovic <milos.bugarinovic@gmail.com> (http://milos.bugarinovic.com)",
  "bin": {
    "vision": "./bin/start.js"
  },
  "bugs": {
    "url": "https://github.com/beecode-rs/msh-vision/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@beecode/msh-node-app": "1.0.0",
    "@beecode/msh-node-env": "0.2.4",
    "@beecode/msh-node-log": "2.2.0",
    "@beecode/msh-node-util": "2.2.0-alpha",
    "async-shelljs": "0.1.4",
    "chalk": "4.1.2",
    "change-case": "4.1.2",
    "compare-versions": "4.1.1",
    "dotenv": "10.0.0",
    "fast-json-stable-stringify": "2.1.0",
    "glob": "7.2.0",
    "inquirer": "8.2.0",
    "minimist": "1.2.5",
    "minimist-options": "4.1.0",
    "module-alias": "2.2.2",
    "mz": "2.7.0",
    "recast": "0.20.5",
    "short-hash": "1.0.0",
    "source-map-support": "0.5.21"
  },
  "description": "MicroService Shell helper",
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@commitlint/prompt": "15.0.0",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/exec": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/release-notes-generator": "10.0.2",
    "@types/chalk": "2.2.0",
    "@types/change-case": "2.3.1",
    "@types/compare-versions": "3.3.0",
    "@types/dotenv": "8.2.0",
    "@types/fast-json-stable-stringify": "2.0.0",
    "@types/glob": "7.2.0",
    "@types/inquirer": "8.1.3",
    "@types/jest": "27.0.3",
    "@types/minimist": "1.2.2",
    "@types/minimist-options": "3.0.0",
    "@types/module-alias": "2.0.1",
    "@types/mz": "2.7.4",
    "@types/node": "16.11.9",
    "@types/shelljs": "0.8.9",
    "@types/source-map-support": "0.5.4",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "codecov": "3.8.2",
    "commitizen": "4.2.4",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "markdown-toc": "1.2.0",
    "nyc": "15.1.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "replace-json-property": "1.8.0",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.0",
    "ts-cleaner": "1.0.5",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "tsc-watch-2": "0.0.2",
    "tsconfig-lint": "0.12.0",
    "typescript": "4.5.2",
    "typescript-parser": "2.6.1"
  },
  "files": [
    "dist",
    "bin",
    "package-lock.json"
  ],
  "homepage": "https://github.com/beecode-rs/msh-vision#readme",
  "keywords": [],
  "license": "mit",
  "main": "./dist/index.js",
  "name": "@beecode/msh-vision",
  "repository": {
    "type": "git",
    "url": "https://github.com/beecode-rs/msh-vision.git"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "diff": "^3.5.0",
    "json-schema": "^0.4.0",
    "minimist": "^0.2.1"
  },
  "scripts": {
    "audit:fix": "npx --yes npm-force-resolutions && npm i",
    "build": "npm run clean && npm run tsc",
    "build:watch": "npm run tsc:watch -- --preserveWatchOutput --onSuccess \"npm run tsc:cleaner\"",
    "build:watch:debug": "npm run build:watch -- --onFirstSuccess \"npm run serve:debug\"",
    "build:watch:serve": "npm run build:watch -- --onFirstSuccess \"npm run serve\"",
    "bump-version": "rjp package.json version",
    "clean": "rimraf ./dist/*",
    "commit": "git-cz",
    "coverage": "nyc npm run test:unit",
    "coverage:publish": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "debug": "npm run build:watch:debug",
    "doc:diagram": "npm run doc:diagram:model && npm run doc:diagram:no-model",
    "doc:diagram:full": "vision --destName=vision-full --printIgnoreExternal --printIgnoreTypes --printSimplifyEntities=\"[[\\\"src/model\\\",\\\"MODEL\\\"]]\" --printIgnorePaths=\"src/util,src/module,src/index.ts,src/enum,src/app,src/service/cli,src/model\"",
    "doc:diagram:model": "vision --src=src/model --destName=vision-model",
    "doc:diagram:no-model": "vision --printIgnoreExternal --printIgnoreTypes --printIgnorePaths=\"src/util,src/module,src/index.ts,src/enum,src/app,src/service/cli,src/model\"",
    "doc:toc": "npx markdown-toc -i ./README.md",
    "eslint": "eslint . --ext .ts --quiet",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --check \"./src/**/*.{ts,json}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{ts,json}\"",
    "semantic-release": "semantic-release",
    "semantic-release:check": "npm run semantic-release -- --dry-run --no-ci",
    "start": "node ./dist/index.js",
    "start:ts": "ts-node ./src/index.ts",
    "test": "npm run test:unit && npm run test:int",
    "test:int": "#mocha ./src/test/{**/,/}*.ts",
    "test:unit": "#jest",
    "tsc": "tsc -p ./tsconfig-build.json",
    "tsc:check": "npm run tsc -- --noEmit",
    "tsc:cleaner": "ts-cleaner --dist dist",
    "tsc:time": "npm run tsc -- --diagnostics",
    "tsc:watch": "tsc-watch -p tsconfig-build.json",
    "watch": "npm run build:watch"
  },
  "version": "0.0.2"
}
